{
    "manifest_version": 2,
    "name": "NetcoolBoi",
    "description": "This extension allows you to auto input generic text into the 'Journaling' textbox of NetCool.",
    "version": "1.1",
    "homepage_url": "https://github.com/Gigoo25/NetcoolAssistant",
    "browser_action": {
        "default_icon": "favicon.png",
        "default_title": "NetcoolBoi"
    },
    "icons": { "16": "favicon.png",
           "48": "favicon.png",
          "128": "favicon.png" },
    "content_scripts": [{
        "matches": ["https://10.6.10.31:16311/*"],
        "js":      ["scripts/content.js", "scripts/jquery.min.js"]
    }],
    "permissions": [
        "activeTab"
    ]
}
